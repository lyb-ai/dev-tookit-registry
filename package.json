    "docs:build": "pnpm docs:generate && vitepress build docs",
    "docs:preview": "vitepress preview docs"
  },
  "peerDependencies": {
    "react": ">=16.8.0"
  },
  "devDependencies": {
    "@types/inquirer": "^9.0.9",
    "@types/node": "^22.19.3",
    "@types/react": "^18.2.0",
    "inquirer": "9.0.0",
    "react": "^18.2.0",
    "serve": "^14.2.5",
    "tsx": "^4.21.0",
    "typescript": "^5.0.0",
    "vitepress": "^1.6.4",
    "vue": "^3.5.26"
  }
}
