{
  "name": "@dev-tookit/registry",
  "version": "1.0.0",
  "private": true,
  "description": "Component registry for dev-tookit",
  "author": "lynyubin",
  "license": "MIT",
  "keywords": [
    "react",
    "hooks",
    "utils",
    "dev-tookit"
  ],
  "scripts": {
    "typecheck": "tsc --noEmit",
    "build:index": "tsx scripts/build-index.ts",
    "newTool": "tsx scripts/new-tool.ts",
    "start": "serve . --cors",
    "docs:generate": "tsx scripts/generate-docs.ts",
